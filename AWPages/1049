<tr class='row-0'><td class='big'>2.7</td><td class='big'><a href="#rapid8103">Big Bend Falls</a></td><td class=big>5.1</td><td><img alt="Waterfall " src="/resources/images/rapid_icon/rapid_icon_waterfall.gif"><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
<tr class='row-1'><td class='big'>4.5</td><td class='big'><a href="#rapid6529">Rock-in the-Hole-in the-Wall</a></td><td class=big>IV+</td><td><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
<tr class='row-0'><td class='big'>5.2</td><td class='big'><a href="#rapid6534">Maytag</a></td><td class=big>5.0</td><td><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
<tr class='row-1'><td class='big'>5.6</td><td class='big'><a href="#rapid6103">Harvey Wallbanger</a></td><td class=big>IV</td><td><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
<tr class='row-0'><td class='big'>5.8</td><td class='big'><a href="#rapid8106">Upper Big Hairy Bastard</a></td><td class=big>IV</td><td><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
