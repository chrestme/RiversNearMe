<tr class='row-1'><td class='big'>2.3</td><td class='big'><a href="#rapid2653">Swain Lowell Dam</a></td><td class=big>III+</td><td><img alt="Portage " src="/resources/images/rapid_icon/rapid_icon_portage.gif"><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
<tr class='row-0'><td class='big'>3.2</td><td class='big'><a href="#rapid1452">Last Class III</a></td><td class=big>III</td><td><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
