<tr class='row-1'><td class='big'>14.0</td><td class='big'><a href="#rapid101435">Mokelumne Falls</a></td><td class=big>V</td><td><img alt="Portage " src="/resources/images/rapid_icon/rapid_icon_portage.gif"><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"></td></tr>
<tr class='row-0'><td class='big'>14.7</td><td class='big'><a href="#rapid101434">Gauging Weir</a></td><td class=big>N/A</td><td><img alt="Portage " src="/resources/images/rapid_icon/rapid_icon_portage.gif"><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"></td></tr>
