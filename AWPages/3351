<tr class='row-0'><td class='big'>1.6</td><td class='big'><a href="#rapid2678">Big Boy</a></td><td class=big>5.3</td><td><img alt="Portage " src="/resources/images/rapid_icon/rapid_icon_portage.gif"><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"><img alt="Waterfall " src="/resources/images/rapid_icon/rapid_icon_waterfall.gif"><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
<tr class='row-1'><td class='big'>1.6</td><td class='big'><a href="#rapid2684">Super Collider</a></td><td class=big>5.2</td><td><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"><img alt="Waterfall " src="/resources/images/rapid_icon/rapid_icon_waterfall.gif"></td></tr>
