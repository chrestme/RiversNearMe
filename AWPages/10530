<tr class='row-1'><td class='big'>1.6</td><td class='big'><a href="#rapid101653">Half Diagonal Ledge</a></td><td class=big>II</td><td><img alt="Playspot " src="/resources/images/rapid_icon/rapid_icon_playspot.gif"><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
<tr class='row-0'><td class='big'>1.7</td><td class='big'><a href="#rapid102317">Creekwide Strainer</a></td><td class=big>N/A</td><td><img alt="Portage " src="/resources/images/rapid_icon/rapid_icon_portage.gif"><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"></td></tr>
