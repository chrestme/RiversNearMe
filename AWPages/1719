<tr class='row-1'><td class='big'>3.3</td><td class='big'><a href="#rapid1850">The Sieve</a></td><td class=big>III</td><td><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
<tr class='row-0'><td class='big'>3.4</td><td class='big'><a href="#rapid1851">Jake's Ledge Hole</a></td><td class=big>III+</td><td><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
<tr class='row-1'><td class='big'>3.5</td><td class='big'><a href="#rapid1853">Canyon Rapid a.k.a. Moonshoot</a></td><td class=big>III</td><td><img alt="Playspot " src="/resources/images/rapid_icon/rapid_icon_playspot.gif"><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
