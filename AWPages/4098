				require(['dojo/dom','dojo/ready','dojo/_base/xhr'],function(dom,ready,xhr){
					
			ready(
				function(){
					var roundNumber = function(num, dec) {
						var result = Math.round(num*Math.pow(10,dec))/Math.pow(10,dec);
						return result;
					}
						var clienttime = ((new Date().getTime()) - wh2timer)/1000;
						var servertime = (0.26337790489197);
						var servermemory = (20065168)/(1024*1024);
						dom.byId('client-time').innerHTML=(clienttime+" sec. client ");
						dom.byId('server-time').innerHTML=(servertime+" sec. server ");
						dom.byId('server-time').innerHTML+= (servermemory.toFixed(2)+" MB");
						if(!Math.floor(Math.random()*101))
						{
							var url = document.location;
							var data = {server_time: servertime, client_time: clienttime, url:url};
							xhr.post({
								url: '/content/Performance/update/',
								content: data	});
							
						}
					}
				);						
					
					
				});
