<tr class='row-1'><td class='big'>51.6</td><td class='big'><a href="#rapid5316">Upper Elevator Rapids</a></td><td class=big>II</td><td><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
<tr class='row-0'><td class='big'>51.8</td><td class='big'><a href="#rapid5317">Lower Elevator Rapids</a></td><td class=big>II+</td><td><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
<tr class='row-1'><td class='big'>52.4</td><td class='big'><a href="#rapid5318">Sandy Beach Access</a></td><td class=big></td><td><img alt="Takeout " src="/resources/images/rapid_icon/rapid_icon_takeout.gif"><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
<tr class='row-0'><td class='big'>53.8</td><td class='big'><a href="#rapid5319">Sherars Falls</a></td><td class=big></td><td><img alt="Portage " src="/resources/images/rapid_icon/rapid_icon_portage.gif"><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"><img alt="Waterfall " src="/resources/images/rapid_icon/rapid_icon_waterfall.gif"><img alt="Photo " src="/resources/images/rapid_icon/rapid_icon_photo.gif"></td></tr>
