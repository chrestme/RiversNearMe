<tr class='row-0'><td class='big'>3.1</td><td class='big'><a href="#rapid102544">Up Against the Wall (UAW)</a></td><td class=big>II</td><td><img alt="Playspot " src="/resources/images/rapid_icon/rapid_icon_playspot.gif"></td></tr>
<tr class='row-1'><td class='big'>3.4</td><td class='big'><a href="#rapid5692">Railroad Rips (RRR)</a></td><td class=big>II</td><td><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"><img alt="Playspot " src="/resources/images/rapid_icon/rapid_icon_playspot.gif"></td></tr>
