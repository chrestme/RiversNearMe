<tr class='row-1'><td class='big'>1.3</td><td class='big'><a href="#rapid100380">Shotgun</a></td><td class=big>III</td><td></td></tr>
<tr class='row-0'><td class='big'>1.5</td><td class='big'><a href="#rapid100385">Door #4</a></td><td class=big>IV</td><td><img alt="Hazard " src="/resources/images/rapid_icon/rapid_icon_hazard.gif"></td></tr>
